<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="/css/LoginFormStyle.css" />
    <link rel="stylesheet" href="/css/LoginFormStyleguide.css" />
    <title>Tamna Login</title>
  </head>

  <body>
    <div class="LoginForm">
      <div class="BodyLogin">
        <div class="Header_Login_Container">
          <div class="Logo_Box">
            <img
              class="Logo"
              src="/img/Tamna_Logo.jpg"
              width="170px"
              ,
              height="170px"
            />
          </div>
        </div>
        <div class="Login_Input_Container">
          <label for="username">
            <input type="text" name="username" placeholder="ID" />
          </label>
          <label for="password">
            <input type="password" name="password" placeholder="PW" />
          </label>
          <button type="submit" class="btnLogin">Login</button>
        </div>
        <div class="Login_ExtraFuction_Container">
          <h3>Find ID</h3>
          <div class="smallFontBox">|</div>
          <h3>Find PW</h3>
          <div class="smallFontBox">|</div>
          <h3><a href="/NewJoin">Join New</a></h3>
        </div>
      </div>
    </div>
  </body>
</html>
